{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Redis": [
    {
      "Name": "Yande",
      "Ip": "116.62.214.239:6379",
      "Port": 6379,
      "Password": "",
      "Timeout": 30,
      "Db": 3
    }
  ],
  "Test": {
    "A": "Hello",
    "B": {
      "C": "World"
    }
  },

  "ES": [
    {
      "Index": "wesky",
      "Url": "http://localhost:9200"
    }
  ],
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug", //最小日志记录级别
      "Override": { //系统日志最小记录级别
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }, //输出到控制台
      {
        "Name": "Async", //异步写入日志
        "Args": {
          "configure": [
            {
              "Name": "File", //输出文件
              "Args": {
                "path": "log/log.txt",
                "outputTemplate": "{NewLine}Date:{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{NewLine}LogLevel:{Level}{NewLine}Class:{SourceContext}{NewLine}Message:{Message}{NewLine}{Exception}",
                "rollingInterval": "3" //日志文件生成精度：1：年  2：月 3：日 4：小时
              }
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
